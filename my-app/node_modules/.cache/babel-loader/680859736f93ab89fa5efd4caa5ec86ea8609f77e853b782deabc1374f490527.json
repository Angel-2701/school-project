{"ast":null,"code":"export default {\n  name: 'FilesDialog',\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: 'Documentos'\n    }\n  },\n  data() {\n    return {\n      seguimientos: [],\n      asesorias: [],\n      archivos: {},\n      archivosDetails: {}\n    };\n  },\n  computed: {\n    dialogVisible: {\n      get() {\n        return this.isOpen;\n      },\n      set(value) {\n        this.$emit('update:isOpen', value);\n      }\n    }\n  },\n  watch: {\n    dialogVisible(newVal) {\n      if (newVal) {\n        this.loadFiles();\n      }\n    }\n  },\n  methods: {\n    loadFiles() {\n      this.loadSeguimientos();\n      this.loadAsesorias();\n      this.loadArchivos();\n    },\n    loadSeguimientos() {\n      const seguimientoIds = [this.student.seguimiento1, this.student.seguimiento2, this.student.seguimiento3].filter(Boolean);\n      this.fetchFiles('uploads', seguimientoIds).then(files => {\n        this.seguimientos = files;\n      });\n    },\n    loadAsesorias() {\n      const asesoriaIds = this.student.archivosAsesorias || [];\n      if (asesoriaIds.length > 0) {\n        this.fetchFiles('asesoria', asesoriaIds).then(files => {\n          this.asesorias = files;\n        });\n      } else {\n        this.asesorias = [];\n      }\n    },\n    loadArchivos() {\n      const archivos = this.student.archivos || {};\n      const archivoIds = Object.values(archivos);\n      this.archivos = archivos;\n      this.fetchFiles('uploads', archivoIds).then(files => {\n        this.archivosDetails = files.reduce((acc, file) => {\n          acc[file._id] = file;\n          return acc;\n        }, {});\n      });\n    },\n    closeDialog() {\n      this.dialogVisible = false;\n    },\n    async fetchFiles(collection, fileIds) {\n      try {\n        const files = await Promise.all(fileIds.map(async fileId => {\n          const response = await fetch(`http://localhost:3000/files/${collection}/${fileId}`);\n          if (!response.ok) {\n            throw new Error(`Failed to fetch file with ID ${fileId}`);\n          }\n          const fileData = await response.json();\n          return {\n            _id: fileId,\n            filename: fileData.filename,\n            data: fileData.data\n          };\n        }));\n        return files;\n      } catch (error) {\n        console.error('Error fetching files:', error);\n        return [];\n      }\n    },\n    downloadFile(file) {\n      if (file && file.data) {\n        const link = document.createElement('a');\n        link.href = `data:application/pdf;base64,${file.data}`;\n        link.download = file.filename;\n        link.click();\n      } else {\n        console.error('File data is not available');\n      }\n    },\n    downloadArchivo(fileId) {\n      const file = this.archivosDetails[fileId];\n      if (file) {\n        this.downloadFile(file);\n      } else {\n        console.error(`File with ID ${fileId} not found in archivosDetails`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","props","isOpen","type","Boolean","required","student","Object","title","String","default","data","seguimientos","asesorias","archivos","archivosDetails","computed","dialogVisible","get","set","value","$emit","watch","newVal","loadFiles","methods","loadSeguimientos","loadAsesorias","loadArchivos","seguimientoIds","seguimiento1","seguimiento2","seguimiento3","filter","fetchFiles","then","files","asesoriaIds","archivosAsesorias","length","archivoIds","values","reduce","acc","file","_id","closeDialog","collection","fileIds","Promise","all","map","fileId","response","fetch","ok","Error","fileData","json","filename","error","console","downloadFile","link","document","createElement","href","download","click","downloadArchivo"],"sources":["/Users/angel/school-project/my-app/src/components/FilesDialog.vue"],"sourcesContent":["<template>\n  <v-dialog v-model=\"dialogVisible\" max-width=\"600\">\n    <v-card>\n      <v-card-title>\n        <span class=\"headline\">{{ title }}</span>\n        <v-spacer></v-spacer>\n        <v-btn icon @click=\"closeDialog\">\n          <v-icon>mdi-close</v-icon>\n        </v-btn>\n      </v-card-title>\n      <v-card-text>\n        <div>\n          <h3>Seguimientos</h3>\n          <v-list v-if=\"seguimientos.length > 0\">\n            <v-list-item\n              v-for=\"seguimiento in seguimientos\"\n              :key=\"seguimiento._id\"\n            >\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(seguimiento)\">{{\n                  seguimiento.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n          <div v-else>No Seguimientos found</div>\n        </div>\n        <div>\n          <h3>Asesorias</h3>\n          <v-list v-if=\"asesorias.length > 0\">\n            <v-list-item v-for=\"asesoria in asesorias\" :key=\"asesoria._id\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(asesoria)\">{{\n                  asesoria.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n          <div v-else>No Asesorias found</div>\n        </div>\n        <div>\n          <h3>Archivos</h3>\n          <v-list v-if=\"Object.keys(archivos).length > 0\">\n            <v-list-item v-for=\"(fileId, fileName) in archivos\" :key=\"fileId\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadArchivo(fileId)\">{{\n                  fileName\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n          <div v-else>No Archivos found</div>\n        </div>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-2\" text @click=\"closeDialog\">Close</v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nexport default {\n  name: 'FilesDialog',\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: 'Documentos'\n    }\n  },\n  data () {\n    return {\n      seguimientos: [],\n      asesorias: [],\n      archivos: {},\n      archivosDetails: {}\n    }\n  },\n  computed: {\n    dialogVisible: {\n      get () {\n        return this.isOpen\n      },\n      set (value) {\n        this.$emit('update:isOpen', value)\n      }\n    }\n  },\n  watch: {\n    dialogVisible (newVal) {\n      if (newVal) {\n        this.loadFiles()\n      }\n    }\n  },\n  methods: {\n    loadFiles () {\n      this.loadSeguimientos()\n      this.loadAsesorias()\n      this.loadArchivos()\n    },\n    loadSeguimientos () {\n      const seguimientoIds = [\n        this.student.seguimiento1,\n        this.student.seguimiento2,\n        this.student.seguimiento3\n      ].filter(Boolean)\n      this.fetchFiles('uploads', seguimientoIds).then((files) => {\n        this.seguimientos = files\n      })\n    },\n    loadAsesorias () {\n      const asesoriaIds = this.student.archivosAsesorias || []\n      if (asesoriaIds.length > 0) {\n        this.fetchFiles('asesoria', asesoriaIds).then((files) => {\n          this.asesorias = files\n        })\n      } else {\n        this.asesorias = []\n      }\n    },\n    loadArchivos () {\n      const archivos = this.student.archivos || {}\n      const archivoIds = Object.values(archivos)\n      this.archivos = archivos\n      this.fetchFiles('uploads', archivoIds).then((files) => {\n        this.archivosDetails = files.reduce((acc, file) => {\n          acc[file._id] = file\n          return acc\n        }, {})\n      })\n    },\n    closeDialog () {\n      this.dialogVisible = false\n    },\n    async fetchFiles (collection, fileIds) {\n      try {\n        const files = await Promise.all(\n          fileIds.map(async (fileId) => {\n            const response = await fetch(\n              `http://localhost:3000/files/${collection}/${fileId}`\n            )\n            if (!response.ok) {\n              throw new Error(`Failed to fetch file with ID ${fileId}`)\n            }\n            const fileData = await response.json()\n            return {\n              _id: fileId,\n              filename: fileData.filename,\n              data: fileData.data\n            }\n          })\n        )\n        return files\n      } catch (error) {\n        console.error('Error fetching files:', error)\n        return []\n      }\n    },\n    downloadFile (file) {\n      if (file && file.data) {\n        const link = document.createElement('a')\n        link.href = `data:application/pdf;base64,${file.data}`\n        link.download = file.filename\n        link.click()\n      } else {\n        console.error('File data is not available')\n      }\n    },\n    downloadArchivo (fileId) {\n      const file = this.archivosDetails[fileId]\n      if (file) {\n        this.downloadFile(file)\n      } else {\n        console.error(`File with ID ${fileId} not found in archivosDetails`)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.headline {\n  font-weight: bold;\n}\n.v-list-item-title {\n  cursor: pointer;\n}\n</style>\n"],"mappings":"AAwEA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,MAAM;MACZF,Q