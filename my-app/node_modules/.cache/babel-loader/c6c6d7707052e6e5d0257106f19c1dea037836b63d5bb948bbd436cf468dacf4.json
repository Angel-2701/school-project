er.correo }}\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col cols=\"4\">\n                  <strong>Project:</strong>\n                </v-col>\n                <v-col cols=\"8\">\n                  {{ user.project }}\n                </v-col>\n              </v-row>\n              <!-- Add more fields as needed -->\n            </v-col>\n          </v-row>\n        </v-container>\n      </v-card-text>\n    </v-card>\n    <v-card v-else>\n      <v-card-title>Loading...</v-card-title>\n    </v-card>\n  </v-container>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  data () {\n    return {\n      user: null\n    }\n  },\n  beforeRouteEnter (to, from, next) {\n    const userId = parseFloat(to.params.userId)\n    axios\n      .get(`http://localhost:3000/users/${userId}`)\n      .then((response) => {\n        next((vm) => {\n          vm.user = response.data\n        })\n      })\n      .catch((error) => {\n        console.error('Error fetching user details:', error)\n        next(false) // Prevent component from rendering\n      })\n  }\n}\n</script>\n\n<style scoped>\n/* Add any necessary styling */\n</style>\n"],"mappings":"AA+DA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,gBAAeA,CAAGC,EAAE,EAAEC,IAAI,EAAEC,IAAI,EAAE;IAChC,MAAMC,MAAK,GAAIC,UAAU,CAACJ,EAAE,CAACK,MAAM,CAACF,MAAM;IAC1CP,KAAI,CACDU,GAAG,CAAE,+BAA8BH,MAAO,EAAC,EAC3CI,IAAI,CAAEC,QAAQ,IAAK;MAClBN,IAAI,CAAEO,EAAE,IAAK;QACXA,EAAE,CAACX,IAAG,GAAIU,QAAQ,CAACX,IAAG;MACxB,CAAC;IACH,CAAC,EACAa,KAAK,CAAEC,KAAK,IAAK;MAChBC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK;MACnDT,IAAI,CAAC,KAAK,GAAE;IACd,CAAC;EACL;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}