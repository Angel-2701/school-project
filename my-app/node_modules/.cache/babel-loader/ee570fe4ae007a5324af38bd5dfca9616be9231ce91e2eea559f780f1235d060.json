etchAsesorias();\n    this.fetchFileData();\n  },\n  computed: {\n    currentAsesoria() {\n      return this.asesorias[this.nextEnabledIndex] || {};\n    },\n    leftAsesorias() {\n      const half = Math.ceil(this.asesorias.length / 2);\n      return this.asesorias.slice(0, half);\n    },\n    rightAsesorias() {\n      const half = Math.ceil(this.asesorias.length / 2);\n      return this.asesorias.slice(half);\n    }\n  }\n};","map":{"version":3,"names":["axios","JSZip","saveAs","data","drawer","sidebarItems","title","icon","userName","localStorage","getItem","numberOfAsesorias","asesorias","JSON","parse","userId","nextEnabledIndex","methods","navigate","index","$router","push","logout","removeItem","initializeAsesorias","Array","from","length","v","i","id","file","submitted","findIndex","asesoria","submitFile","formData","FormData","append","response","post","headers","status","filename","setItem","stringify","a","window","alert","console","log","error","deleteFile","fileId","delete","then","catch","fetchAsesorias","get","userData","fetchFileData","files","forEach","downloadAllFiles","zip","byteString","atob","ab","ArrayBuffer","ia","Uint8Array","charCodeAt","generateAsync","type","content","mounted","computed","currentAsesoria","leftAsesorias","half","Math","ceil","slice","rightAsesorias"],"sources":["/Users/angel/school-project/my-app/src/components/AsesoriasAlumno.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-container>\n      <!-- Top Bar -->\n      <v-app-bar app color=\"blue darken-2\" dark>\n        <!-- Burger menu icon -->\n        <v-btn icon @click=\"drawer = !drawer\">\n          <v-icon>mdi-menu</v-icon>\n        </v-btn>\n        <!-- Title -->\n        <v-toolbar-title>{{ userName }}</v-toolbar-title>\n        <v-spacer></v-spacer>\n        <!-- Logout button -->\n        <v-btn icon @click=\"logout\">\n          <v-icon>mdi-logout</v-icon>\n        </v-btn>\n      </v-app-bar>\n\n      <!-- Sidebar -->\n      <v-navigation-drawer app v-model=\"drawer\" color=\"blue darken-2\" dark>\n        <v-list dense>\n          <v-list-item\n            v-for=\"(item, index) in sidebarItems\"\n            :key=\"index\"\n            @click=\"navigate(index)\"\n          >\n            <v-list-item-action>\n              <v-icon>{{ item.icon }}</v-icon>\n            </v-list-item-action>\n            <v-list-item-content>\n              <v-list-item-title>{{ item.title }}</v-list-item-title>\n            </v-list-item-content>\n          </v-list-item>\n        </v-list>\n      </v-navigation-drawer>\n\n      <!-- Main Content -->\n      <v-main>\n        <v-col cols=\"12\" style=\"max-width: 800px; margin: 0 auto\">\n          <v-card class=\"mx-auto\" max-width=\"600\">\n            <v-card-title class=\"blue darken-2 white--text\"\n              >Asesorías</v-card-title\n            >\n            <v-card-text>\n              <v-row>\n                <v-col cols=\"12\">\n                  <p>Number of Asesorías: {{ numberOfAsesorias }}</p>\n                </v-col>\n                <v-divider class=\"my-4\"></v-divider>\n                <v-row>\n                  <v-col\n                    v-for=\"(asesoria, index) in leftAsesorias\"\n                    :key=\"asesoria.id\"\n                    cols=\"6\"\n                  >\n                    <v-row>\n                      <v-col cols=\"12\">\n                        <v-file-input\n                          v-model=\"asesoria.file\"\n                          :label=\"'Upload File for Asesoría ' + (index + 1)\"\n                          prepend-icon=\"mdi-upload\"\n                          :disabled=\"index !== nextEnabledIndex\"\n                          accept=\"application/pdf\"\n                        ></v-file-input>\n                      </v-col>\n                      <!-- Delete button -->\n                      <v-col v-if=\"asesoria.submitted\" cols=\"12\">\n                        <v-btn color=\"red\" fab small @click=\"deleteFile(index)\">\n                          <v-icon>mdi-delete</v-icon>\n                        </v-btn>\n                      </v-col>\n                    </v-row>\n                  </v-col>\n                  <v-col\n                    v-for=\"(asesoria, index) in rightAsesorias\"\n                    :key=\"asesoria.id\"\n                    cols=\"6\"\n                  >\n                    <v-row>\n                      <v-col cols=\"12\">\n                        <v-file-input\n                          v-model=\"asesoria.file\"\n                          :label=\"\n                            'Upload File for Asesoría ' +\n                            (index + leftAsesorias.length + 1)\n                          \"\n                          prepend-icon=\"mdi-upload\"\n                          :disabled=\"\n                            index !== nextEnabledIndex - leftAsesorias.length\n                          \"\n                          accept=\"application/pdf\"\n                        ></v-file-input>\n                      </v-col>\n                      <!-- Delete button -->\n                      <v-col v-if=\"asesoria.submitted\" cols=\"12\">\n                        <v-btn\n                          color=\"red\"\n                          fab\n                          small\n                          @click=\"deleteFile(index + leftAsesorias.length)\"\n                        >\n                          <v-icon>mdi-delete</v-icon>\n                        </v-btn>\n                      </v-col>\n                    </v-row>\n                  </v-col>\n                </v-row>\n              </v-row>\n            </v-card-text>\n            <v-card-actions>\n              <v-btn\n                color=\"blue darken-2\"\n                @click=\"submitFile\"\n                :disabled=\"!currentAsesoria.file\"\n              >\n                Submit Asesoría {{ nextEnabledIndex + 1 }}\n              </v-btn>\n              <v-btn color=\"green darken-2\" @click=\"downloadAllFiles\"\n                >Download All Files</v-btn\n              >\n            </v-card-actions>\n          </v-card>\n        </v-col>\n      </v-main>\n    </v-container>\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport JSZip from \"jszip\";\nimport { saveAs } from \"file-saver\";\n\nexport default {\n  data() {\n    return {\n      drawer: false,\n      sidebarItems: [\n        { title: \"Proyecto\", icon: \"mdi-folder-outline\" },\n        { title: \"Asesorias\", icon: \"mdi-account-plus-outline\" },\n      ],\n      userName: localStorage.getItem(\"userName\"),\n      numberOfAsesorias: 0,\n      asesorias: JSON.parse(localStorage.getItem(\"asesorias\")) || [],\n      userId: localStorage.getItem(\"id\"),\n      nextEnabledIndex: 0, // Index of the next enabled file input field\n    };\n  },\n  methods: {\n    navigate(index) {\n      switch (index) {\n        case 0:\n          this.$router.push(\"/student\");\n          break;\n        case 1:\n          this.$router.push(\"/student/asesorias\");\n          break;\n        default:\n          break;\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"token\");\n      this.$router.push(\"/login\");\n    },\n    initializeAsesorias() {\n      this.asesorias = Array.from(\n        { length: this.numberOfAsesorias },\n        (v, i) => ({\n          id: i + 1,\n          file: this.asesorias[i]?.file || null,\n          submitted: this.asesorias[i]?.submitted || false, // Set submitted status based on existing data\n        })\n      );\n      // Find the next index to enable\n      this.nextEnabledIndex = this.asesorias.findIndex(\n        (asesoria) => !asesoria.submitted\n      );\n      if (this.nextEnabledIndex === -1) {\n        this.nextEnabledIndex = this.numberOfAsesorias;\n      }\n    },\n    async submitFile() {\n      const index = this.nextEnabledIndex;\n      const formData = new FormData();\n      const asesoria = this.asesorias[index];\n\n      if (asesoria.file && !asesoria.submitted) {\n        formData.append(\"file\", asesoria.file);\n\n        try {\n          const response = await axios.post(\n            \"http://localhost:3000/upload/single\",\n            formData,\n            {\n              headers: {\n                \"Content-Type\": \"multipart/form-data\",\n              },\n            }\n          );\n\n          if (response.status === 201) {\n            const file = response.data.file;\n            asesoria.submitted = true;\n            asesoria.file = {\n              id: file.id,\n              filename: file.filename,\n            };\n\n            // Save the updated asesorias data in localStorage\n            localStorage.setItem(\"asesorias\", JSON.stringify(this.asesorias));\n\n            // Find and set the next enabled index\n            this.nextEnabledIndex = this.asesorias.findIndex(\n              (a) => !a.submitted\n            );\n            if (this.nextEnabledIndex === -1) {\n              this.nextEnabledIndex = this.numberOfAsesorias;\n            }\n\n            window.alert(\"File uploaded successfully!\"); // Display alert\n            console.log(\"File uploaded successfully!\");\n          } else {\n            console.error(\"Failed to upload file.\");\n          }\n        } catch (error) {\n          console.error(\"Error uploading file:\", error);\n        }\n      }\n    },\n    deleteFile(index) {\n      const fileId = this.asesorias[index].file.id;\n      axios\n        .delete(`http://localhost:3000/files/${fileId}`)\n        .then((response) => {\n          if (response.status === 200) {\n            this.asesorias[index].submitted = false;\n            // Set file property to null instead of an empty object\n            this.asesorias[index].file = null;\n            localStorage.setItem(\"asesorias\", JSON.stringify(this.asesorias));\n            window.alert(\"File deleted successfully!\");\n            // Update the next enabled index\n            this.nextEnabledIndex = index;\n          } else {\n            console.error(\"Failed to delete file.\");\n          }\n        })\n        .catch((error) => {\n          console.error(\"Error deleting file:\", error);\n        });\n    },\n    async fetchAsesorias() {\n      try {\n        const response = await axios.get(\n          `http://localhost:3000/users/${this.userId}`\n        );\n        const userData = response.data;\n        this.numberOfAsesorias = userData.asesorias;\n        this.initializeAsesorias(); // Initialize asesorias based on fetched number\n      } catch (error) {\n        console.error(\"Error fetching asesorías:\", error);\n      }\n    },\n    async fetchFileData() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/files\");\n        const files = response.data;\n        // Update file objects with their data based on index\n        files.forEach((file, index) => {\n          if (this.asesorias[index]) {\n            this.asesorias[index].file = file;\n          }\n        });\n      } catch (error) {\n        console.error(\"Error fetching file data:\", error);\n      }\n    },\n    async downloadAllFiles() {\n      // Ensure the latest data is fetched before downloading\n      await this.fetchFileData();\n\n      const zip = new JSZip();\n      this.asesorias.forEach((asesoria, index) => {\n        if (asesoria.file && asesoria.file.data) {\n          const byteString = atob(asesoria.file.data);\n          const ab = new ArrayBuffer(byteString.length);\n          const ia = new Uint8Array(ab);\n          for (let i = 0; i < byteString.length; i++) {\n            ia[i] = byteString.charCodeAt(i);\n          }\n          zip.file(`filename_${index + 1}.pdf`, ab);\n        }\n      });\n\n      zip.generateAsync({ type: \"blob\" }).then((content) => {\n        saveAs(content, \"asesorias.zip\");\n      });\n    },\n  },\n  mounted() {\n    this.fetchAsesorias();\n    this.fetchFileDa