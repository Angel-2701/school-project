{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      users: [],\n      sidebarItems: [{\n        title: \"Projects\",\n        icon: \"mdi-folder-outline\"\n      }, {\n        title: \"Users\",\n        icon: \"mdi-account-outline\"\n      }, {\n        title: \"Create Teacher\",\n        icon: \"mdi-teacher\"\n      }],\n      userName: \"John Doe\",\n      drawer: false,\n      editDialog: false,\n      editedUser: {\n        nombre: \"\",\n        apellido: \"\",\n        correo: \"\"\n        // Add more fields as needed\n      },\n      createDialog: false,\n      newUser: {\n        nombre: \"\",\n        apellido: \"\",\n        correo: \"\"\n        // Add more fields as needed\n      }\n    };\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/users\");\n        this.users = response.data;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n    navigate(index) {\n      switch (index) {\n        case 0:\n          this.$router.push(\"/admin\");\n          break;\n        case 1:\n          this.$router.push(\"/users\");\n          break;\n        case 2:\n          this.$router.push(\"/create-teacher\");\n          break;\n        default:\n          break;\n      }\n    },\n    logout() {\n      // Logic to logout the user\n    },\n    editUser(user) {\n      this.editedUser = {\n        ...user\n      }; // Copy user data to editedUser\n      this.editDialog = true;\n    },\n    saveEditedUser() {\n      axios.put(`http://localhost:3000/users/${this.editedUser._id}`, this.editedUser).then(response => {\n        console.log(\"User updated successfully:\", response.data);\n        this.fetchData();\n      }).catch(error => {\n        console.error(\"Error updating user:\", error);\n      });\n      this.editDialog = false;\n    },\n    cancelEdit() {\n      this.editDialog = false;\n    },\n    openCreateDialog() {\n      this.createDialog = true;\n    },\n    saveNewUser() {\n      axios.post(\"http://localhost:3000/register\", this.newUser).then(response => {\n        if (response.status === 201) {\n          console.log(\"User created successfully:\", response.data);\n          this.fetchData();\n        } else {\n          console.error(\"Failed to create user:\", response.data);\n        }\n      }).catch(error => {\n        console.error(\"Error creating user:\", error);\n      });\n      this.createDialog = false;\n    },\n    cancelCreate() {\n      this.createDialog = false;\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["axios","data","users","sidebarItems","title","icon","userName","drawer","editDialog","editedUser","nombre","apellido","correo","createDialog","newUser","methods","fetchData","response","get","error","console","navigate","index","$router","push","logout","editUser","user","saveEditedUser","put","_id","then","log","catch","cancelEdit","openCreateDialog","saveNewUser","post","status","cancelCreate","mounted"],"sources":["/Users/angel/project-system/my-app/src/components/StudentsPage.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-container fluid>\n      <!-- Top Bar -->\n      <v-app-bar app color=\"blue darken-2\" dark>\n        <!-- Burger menu icon -->\n        <v-btn icon @click=\"drawer = !drawer\">\n          <v-icon>mdi-menu</v-icon>\n        </v-btn>\n        <!-- Title -->\n        <v-toolbar-title>{{ userName }}</v-toolbar-title>\n        <v-spacer></v-spacer>\n        <!-- Logout button -->\n        <v-btn icon @click=\"logout\">\n          <v-icon>mdi-logout</v-icon>\n        </v-btn>\n      </v-app-bar>\n\n      <!-- Sidebar -->\n      <v-navigation-drawer app v-model=\"drawer\" color=\"blue darken-2\" dark>\n        <v-list dense>\n          <v-list-item\n            v-for=\"(item, index) in sidebarItems\"\n            :key=\"index\"\n            @click=\"navigate(index)\"\n          >\n            <v-list-item-action>\n              <v-icon>{{ item.icon }}</v-icon>\n            </v-list-item-action>\n            <v-list-item-content>\n              <v-list-item-title>{{ item.title }}</v-list-item-title>\n            </v-list-item-content>\n          </v-list-item>\n        </v-list>\n      </v-navigation-drawer>\n\n      <v-main>\n        <!-- Main content -->\n        <v-col cols=\"10\">\n          <v-card>\n            <!-- Users Data -->\n            <v-data-table v-if=\"users.length > 0\" :items=\"users\" align=\"center\">\n              <template v-slot:top>\n                <v-toolbar flat color=\"blue darken-2\">\n                  <v-toolbar-title>Users</v-toolbar-title>\n                  <v-divider class=\"mx-4\" inset vertical></v-divider>\n                  <v-spacer></v-spacer>\n                  <v-btn icon color=\"blue darken-2\" @click=\"openCreateDialog\">\n                    <v-icon>mdi-plus</v-icon>\n                  </v-btn>\n                </v-toolbar>\n              </template>\n              <template v-slot:item=\"{ item }\">\n                <tr>\n                  <td v-for=\"(value, key) in item\" :key=\"key\">\n                    {{ value }}\n                  </td>\n                  <td>\n                    <v-btn\n                      icon\n                      small\n                      color=\"blue darken-2\"\n                      @click=\"editUser(item)\"\n                      style=\"width: 30px; height: 30px; margin-right: 5px\"\n                    >\n                      <v-icon style=\"font-size: 18px\">mdi-pencil</v-icon>\n                    </v-btn>\n                    <v-btn\n                      icon\n                      small\n                      color=\"red darken-2\"\n                      @click=\"deleteUser(item)\"\n                      style=\"width: 30px; height: 30px; margin-right: 5px\"\n                    >\n                      <v-icon style=\"font-size: 18px\">mdi-delete</v-icon>\n                    </v-btn>\n                  </td>\n                </tr>\n              </template>\n            </v-data-table>\n          </v-card>\n        </v-col>\n      </v-main>\n\n      <!-- Edit Dialog -->\n      <v-dialog v-model=\"editDialog\" max-width=\"500\">\n        <v-card>\n          <v-card-title>Edit User</v-card-title>\n          <v-card-text>\n            <v-text-field\n              v-model=\"editedUser.nombre\"\n              label=\"Name\"\n            ></v-text-field>\n            <v-text-field\n              v-model=\"editedUser.apellido\"\n              label=\"Last Name\"\n            ></v-text-field>\n            <v-text-field\n              v-model=\"editedUser.correo\"\n              label=\"Email\"\n            ></v-text-field>\n            <!-- Add more fields as needed -->\n          </v-card-text>\n          <v-card-actions>\n            <v-btn color=\"blue darken-2\" @click=\"saveEditedUser\">Save</v-btn>\n            <v-btn @click=\"cancelEdit\">Cancel</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n\n      <!-- Create Dialog -->\n      <v-dialog v-model=\"createDialog\" max-width=\"500\">\n        <v-card>\n          <v-card-title>Create New User</v-card-title>\n          <v-card-text>\n            <v-text-field v-model=\"newUser.nombre\" label=\"Name\"></v-text-field>\n            <v-text-field\n              v-model=\"newUser.apellido\"\n              label=\"Last Name\"\n            ></v-text-field>\n            <v-text-field v-model=\"newUser.correo\" label=\"Email\"></v-text-field>\n            <!-- Add more fields as needed -->\n          </v-card-text>\n          <v-card-actions>\n            <v-btn color=\"blue darken-2\" @click=\"saveNewUser\">Save</v-btn>\n            <v-btn @click=\"cancelCreate\">Cancel</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-container>\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      users: [],\n      sidebarItems: [\n        { title: \"Projects\", icon: \"mdi-folder-outline\" },\n        { title: \"Users\", icon: \"mdi-account-outline\" },\n        { title: \"Create Teacher\", icon: \"mdi-teacher\" },\n      ],\n      userName: \"John Doe\",\n      drawer: false,\n      editDialog: false,\n      editedUser: {\n        nombre: \"\",\n        apellido: \"\",\n        correo: \"\",\n        // Add more fields as needed\n      },\n      createDialog: false,\n      newUser: {\n        nombre: \"\",\n        apellido: \"\",\n        correo: \"\",\n        // Add more fields as needed\n      },\n    };\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/users\");\n        this.users = response.data;\n      } catch (error) {\n        console.error(\"Error fetching users:\", error);\n      }\n    },\n\n    navigate(index) {\n      switch (index) {\n        case 0:\n          this.$router.push(\"/admin\");\n          break;\n        case 1:\n          this.$router.push(\"/users\");\n          break;\n        case 2:\n          this.$router.push(\"/create-teacher\");\n          break;\n        default:\n          break;\n      }\n    },\n    logout() {\n      // Logic to logout the user\n    },\n    editUser(user) {\n      this.editedUser = { ...user }; // Copy user data to editedUser\n      this.editDialog = true;\n    },\n    saveEditedUser() {\n      axios\n        .put(\n          `http://localhost:3000/users/${this.editedUser._id}`,\n          this.editedUser\n        )\n        .then((response) => {\n          console.log(\"User updated successfully:\", response.data);\n          this.fetchData();\n        })\n        .catch((error) => {\n          console.error(\"Error updating user:\", error);\n        });\n      this.editDialog = false;\n    },\n    cancelEdit() {\n      this.editDialog = false;\n    },\n    openCreateDialog() {\n      this.createDialog = true;\n    },\n    saveNewUser() {\n      axios\n        .post(\"http://localhost:3000/register\", this.newUser)\n        .then((response) => {\n          if (response.status === 201) {\n            console.log(\"User created successfully:\", response.data);\n            this.fetchData();\n          } else {\n            console.error(\"Failed to create user:\", response.data);\n          }\n        })\n        .catch((error) => {\n          console.error(\"Error creating user:\", error);\n        });\n      this.createDialog = false;\n    },\n    cancelCreate() {\n      this.createDialog = false;\n    },\n  },\n  mounted() {\n    this.fetchData();\n  },\n};\n</script>\n\n<style scoped>\n.v-btn i {\n  color: white;\n}\n</style>\n"],"mappings":";AAsIA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,CACZ;QAAEC,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAqB,CAAC,EACjD;QAAED,KAAK,EAAE,OAAO;QAAEC,IAAI,EAAE;MAAsB,CAAC,EAC/C;QAAED,KAAK,EAAE,gBAAgB;QAAEC,IAAI,EAAE;MAAc,CAAC,CACjD;MACDC,QAAQ,EAAE,UAAU;MACpBC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,KAAK;MACjBC,UAAU,EAAE;QACVC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;QACR;MACF,CAAC;MACDC,YAAY,EAAE,KAAK;MACnBC,OAAO,EAAE;QACPJ,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;QACR;MACF;IACF,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,6BAA6B,CAAC;QAC/D,IAAI,CAAChB,KAAI,GAAIe,QAAQ,CAAChB,IAAI;MAC5B,EAAE,OAAOkB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IAEDE,QAAQA,CAACC,KAAK,EAAE;MACd,QAAQA,KAAK;QACX,KAAK,CAAC;UACJ,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;UAC3B;QACF,KAAK,CAAC;UACJ,IAAI,CAACD,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;UAC3B;QACF,KAAK,CAAC;UACJ,IAAI,CAACD,OAAO,CAACC,IAAI,CAAC,iBAAiB,CAAC;UACpC;QACF;UACE;MACJ;IACF,CAAC;IACDC,MAAMA,CAAA,EAAG;MACP;IAAA,CACD;IACDC,QAAQA,CAACC,IAAI,EAAE;MACb,I