{"ast":null,"code":"export default {\n  name: \"FilesDialog\",\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: \"Documentos\"\n    }\n  },\n  data() {\n    return {\n      files: [],\n      seguimientos: [],\n      asesorias: [] // Example placeholder data for Asesorias\n    };\n  },\n  computed: {\n    dialogVisible: {\n      get() {\n        return this.isOpen;\n      },\n      set(value) {\n        this.$emit(\"update:isOpen\", value);\n      }\n    }\n  },\n  watch: {\n    dialogVisible(newVal) {\n      if (newVal) {\n        this.loadFiles();\n        this.loadSeguimientos();\n        this.loadAsesorias(); // Placeholder function call for Asesorias\n      }\n    }\n  },\n  methods: {\n    loadFiles() {\n      const fileIds = [this.student.file1, this.student.file2];\n      this.fetchFiles(fileIds).then(files => {\n        this.files = files;\n      });\n    },\n    loadSeguimientos() {\n      if (this.student && this.student.seguimiento1 && this.student.seguimiento2 && this.student.seguimiento3) {\n        const seguimientoIds = [this.student.seguimiento1, this.student.seguimiento2, this.student.seguimiento3];\n        this.fetchFiles(seguimientoIds).then(files => {\n          this.seguimientos = files;\n        });\n      }\n    },\n    loadAsesorias() {\n      // Placeholder function to load Asesorias\n      this.asesorias = [{\n        id: 1,\n        title: \"Asesoria 1\"\n      }, {\n        id: 2,\n        title: \"Asesoria 2\"\n      }];\n    },\n    closeDialog() {\n      this.dialogVisible = false;\n    },\n    async fetchFiles(fileIds) {\n      try {\n        const files = await Promise.all(fileIds.map(async fileId => {\n          const response = await fetch(`http://localhost:3000/files/${fileId}`);\n          if (!response.ok) {\n            throw new Error(`Failed to fetch file with ID ${fileId}`);\n          }\n          const fileData = await response.json();\n          return {\n            _id: fileId,\n            filename: fileData.filename,\n            data: fileData.data\n          };\n        }));\n        return files;\n      } catch (error) {\n        console.error(\"Error fetching files:\", error);\n        return [];\n      }\n    },\n    downloadFile(file) {\n      const link = document.createElement(\"a\");\n      link.href = `data:application/pdf;base64,${file.data}`;\n      link.download = file.filename;\n      link.click();\n    }\n  }\n};","map":{"version":3,"names":["name","props","isOpen","type","Boolean","required","student","Object","title","String","default","data","files","seguimientos","asesorias","computed","dialogVisible","get","set","value","$emit","watch","newVal","loadFiles","loadSeguimientos","loadAsesorias","methods","fileIds","file1","file2","fetchFiles","then","seguimiento1","seguimiento2","seguimiento3","seguimientoIds","id","closeDialog","Promise","all","map","fileId","response","fetch","ok","Error","fileData","json","_id","filename","error","console","downloadFile","file","link","document","createElement","href","download","click"],"sources":["/Users/angel/school-project/my-app/src/components/FilesDialog.vue"],"sourcesContent":["<template>\n  <v-dialog v-model=\"dialogVisible\" max-width=\"600\">\n    <v-card>\n      <v-card-title>\n        <span class=\"headline\">{{ title }}</span>\n        <v-spacer></v-spacer>\n        <v-btn icon @click=\"closeDialog\">\n          <v-icon>mdi-close</v-icon>\n        </v-btn>\n      </v-card-title>\n      <v-card-text>\n        <div>\n          <h3>Seguimientos</h3>\n          <v-list>\n            <v-list-item\n              v-for=\"seguimiento in seguimientos\"\n              :key=\"seguimiento._id\"\n            >\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(seguimiento)\">{{\n                  seguimiento.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </div>\n        <div>\n          <h3>Asesorias</h3>\n          <v-list>\n            <v-list-item v-for=\"asesoria in asesorias\" :key=\"asesoria.id\">\n              <v-list-item-content>\n                <v-list-item-title>{{ asesoria.title }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </div>\n        <div>\n          <h3>Archivos</h3>\n          <v-list v-if=\"files.length > 0\">\n            <v-list-item v-for=\"file in files\" :key=\"file._id\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(file)\">{{\n                  file.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </div>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-2\" text @click=\"closeDialog\">Close</v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nexport default {\n  name: \"FilesDialog\",\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true,\n    },\n    student: {\n      type: Object,\n      required: true,\n    },\n    title: {\n      type: String,\n      default: \"Documentos\",\n    },\n  },\n  data() {\n    return {\n      files: [],\n      seguimientos: [],\n      asesorias: [], // Example placeholder data for Asesorias\n    };\n  },\n  computed: {\n    dialogVisible: {\n      get() {\n        return this.isOpen;\n      },\n      set(value) {\n        this.$emit(\"update:isOpen\", value);\n      },\n    },\n  },\n  watch: {\n    dialogVisible(newVal) {\n      if (newVal) {\n        this.loadFiles();\n        this.loadSeguimientos();\n        this.loadAsesorias(); // Placeholder function call for Asesorias\n      }\n    },\n  },\n  methods: {\n    loadFiles() {\n      const fileIds = [this.student.file1, this.student.file2];\n      this.fetchFiles(fileIds).then((files) => {\n        this.files = files;\n      });\n    },\n    loadSeguimientos() {\n      if (\n        this.student &&\n        this.student.seguimiento1 &&\n        this.student.seguimiento2 &&\n        this.student.seguimiento3\n      ) {\n        const seguimientoIds = [\n          this.student.seguimiento1,\n          this.student.seguimiento2,\n          this.student.seguimiento3,\n        ];\n        this.fetchFiles(seguimientoIds).then((files) => {\n          this.seguimientos = files;\n        });\n      }\n    },\n    loadAsesorias() {\n      // Placeholder function to load Asesorias\n      this.asesorias = [\n        { id: 1, title: \"Asesoria 1\" },\n        { id: 2, title: \"Asesoria 2\" },\n      ];\n    },\n    closeDialog() {\n      this.dialogVisible = false;\n    },\n    async fetchFiles(fileIds) {\n      try {\n        const files = await Promise.all(\n          fileIds.map(async (fileId) => {\n            const response = await fetch(\n              `http://localhost:3000/files/${fileId}`\n            );\n            if (!response.ok) {\n              throw new Error(`Failed to fetch file with ID ${fileId}`);\n            }\n            const fileData = await response.json();\n            return {\n              _id: fileId,\n              filename: fileData.filename,\n              data: fileData.data,\n            };\n          })\n        );\n        return files;\n      } catch (error) {\n        console.error(\"Error fetching files:\", error);\n        return [];\n      }\n    },\n    downloadFile(file) {\n      const link = document.createElement(\"a\");\n      link.href = `data:application/pdf;base64,${file.data}`;\n      link.download = file.filename;\n      link.click();\n    },\n  },\n};\n</script>\n\n<style scoped>\n.headline {\n  font-weight: bold;\n}\n.v-list-item-title {\n  cursor: pointer;\n}\n</style>\n"],"mappings":"AAgEA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,MAAM;MACZF,QAAQ,EAAE;IACZ,CAAC;IACDG,KAAK,EAAE;MACLL,IAAI,EAAEM,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,EAAE,CAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAa,EAAE;MACbC,GAAGA,CAAA,EAAG;QACJ,OAAO,IAAI,CAACf,MAAM;MACpB,CAAC;MACDgB,GAAGA,CAACC,KAAK,EAAE;QACT,IAAI,CAACC,KAAK,CAAC,eAAe,EAAED,KAAK,CAAC;MACpC;IACF;EACF,CAAC;EACDE,KAAK,EAAE;IACLL,aAAaA,CAACM,MAAM,EAAE;MACpB,IAAIA,MAAM,EAAE;QACV,IAAI,CAACC,SAAS,CAAC,CAAC;QAChB,IAAI,CAACC,gBAAgB,CAAC,CAAC;QACvB,IAAI,CAACC,aAAa,CAAC,CAAC,EAAE;MACxB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPH,SAASA,CAAA,EAAG;MACV,MAAMI,OAAM,GAAI,CAAC,IAAI,CAACrB,OAAO,CAACsB,KAAK,EAAE,IAAI,CAACtB,OAAO,CAACuB,KAAK,CAAC;MACxD,IAAI,CAACC,UAAU,CAACH,OAAO,CAAC,CAACI,IAAI,CAAEnB,KAAK,IAAK;QACvC,IAAI,CAACA,KAAI,GAAIA,KAAK;MACpB,CAAC,CAAC;IACJ,CAAC;IACDY,gBAAgBA,CAAA,EAAG;MACjB,IACE,IAAI,CAAClB,OAAM,IACX,IAAI,CAACA,OAAO,CAAC0B,YAAW,IACxB,IAAI,CAAC1B,OAAO,CAAC2B,YAAW,IACxB,IAAI,CAAC3B,OAAO,CAAC4B,YAAW,EACxB;QACA,MAAMC,cAAa,GAAI,CACrB,IAAI,CAAC7B,OAAO,CAAC0B,YAAY,EACzB,IAAI,CAAC1B,OAAO,CAAC2B,YAAY,EACzB,IAAI,CAAC3B,OAAO,CAAC4B,YAAY,CAC1B;QACD,IAAI,CAACJ,UAAU,CAACK,cAAc,CAAC,CAACJ,IAAI,CAAEnB,KAAK,IAAK;UAC9C,IAAI,CAACC,YAAW,GAAID,KAAK;QAC3B,CAAC,CAAC;MACJ;IACF,CAAC;IACDa,aAAaA,CAAA,EAAG;MACd;MACA,IAAI,CAACX,SAAQ,GAAI,CACf;QAAEsB,EAAE,EAAE,CAAC;QAAE5B,KAAK,EAAE;MAAa,CAAC,EAC9B;QAAE4B,EAAE,EAAE,CAAC;QAAE5B,KAAK,EAAE;MAAa,CAAC,CAC/B;IACH,CAAC;IACD6B,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACrB,aAAY,GAAI,KAAK;IAC5B,CAAC;IACD,MAAMc,UAAUA,CAACH,OAAO,EAAE;MACxB,IAAI;QACF,MAAMf,KAAI,GAAI,MAAM0B,OAAO,CAACC,GAAG,C