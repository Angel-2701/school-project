{"ast":null,"code":"export default {\n  name: 'FilesDialog',\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: 'Documentos'\n    }\n  },\n  data() {\n    return {\n      files: [],\n      seguimientos: [],\n      asesorias: [],\n      archivos: {}\n    };\n  },\n  computed: {\n    dialogVisible: {\n      get() {\n        return this.isOpen;\n      },\n      set(value) {\n        this.$emit('update:isOpen', value);\n      }\n    }\n  },\n  watch: {\n    dialogVisible(newVal) {\n      if (newVal) {\n        this.loadFiles();\n        this.loadSeguimientos();\n        this.loadAsesorias();\n        this.loadArchivos();\n      }\n    }\n  },\n  methods: {\n    loadFiles() {\n      const fileIds = [this.student.file1, this.student.file2];\n      this.fetchFiles('uploads', fileIds).then(files => {\n        this.files = files;\n      });\n    },\n    loadSeguimientos() {\n      if (this.student && this.student.seguimiento1 && this.student.seguimiento2 && this.student.seguimiento3) {\n        const seguimientoIds = [this.student.seguimiento1, this.student.seguimiento2, this.student.seguimiento3];\n        this.fetchFiles('uploads', seguimientoIds).then(files => {\n          this.seguimientos = files;\n        });\n      }\n    },\n    loadAsesorias() {\n      const asesoriaIds = this.student.archivosAsesorias || [];\n      if (asesoriaIds.length > 0) {\n        this.fetchFiles('asesoria', asesoriaIds).then(files => {\n          this.asesorias = files;\n        });\n      } else {\n        this.asesorias = [];\n      }\n    },\n    loadArchivos() {\n      this.archivos = this.student.archivos || {};\n    },\n    closeDialog() {\n      this.dialogVisible = false;\n    },\n    async fetchFiles(collection, fileIds) {\n      try {\n        const files = await Promise.all(fileIds.map(async fileId => {\n          const response = await fetch(`http://localhost:3000/files/${collection}/${fileId}`);\n          if (!response.ok) {\n            throw new Error(`Failed to fetch file with ID ${fileId}`);\n          }\n          const fileData = await response.json();\n          return {\n            _id: fileId,\n            filename: fileData.filename,\n            data: fileData.data\n          };\n        }));\n        return files;\n      } catch (error) {\n        console.error('Error fetching files:', error);\n        return [];\n      }\n    },\n    downloadFile(file) {\n      const link = document.createElement('a');\n      link.href = `data:application/pdf;base64,${file.data}`;\n      link.download = file.filename;\n      link.click();\n    },\n    async downloadFileById(fileId) {\n      try {\n        const response = await fetch(`http://localhost:3000/files/uploads/${fileId}`);\n        if (!response.ok) {\n          throw new Error(`Failed to fetch file with ID ${fileId}`);\n        }\n        const fileData = await response.json();\n        const link = document.createElement('a');\n        link.href = `data:application/pdf;base64,${fileData.data}`;\n        link.download = fileData.filename;\n        link.click();\n      } catch (error) {\n        console.error('Error downloading file:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","props","isOpen","type","Boolean","required","student","Object","title","String","default","data","files","seguimientos","asesorias","archivos","computed","dialogVisible","get","set","value","$emit","watch","newVal","loadFiles","loadSeguimientos","loadAsesorias","loadArchivos","methods","fileIds","file1","file2","fetchFiles","then","seguimiento1","seguimiento2","seguimiento3","seguimientoIds","asesoriaIds","archivosAsesorias","length","closeDialog","collection","Promise","all","map","fileId","response","fetch","ok","Error","fileData","json","_id","filename","error","console","downloadFile","file","link","document","createElement","href","download","click","downloadFileById"],"sources":["/Users/angel/school-project/my-app/src/components/FilesDialog.vue"],"sourcesContent":["<template>\n  <v-dialog v-model=\"dialogVisible\" max-width=\"600\">\n    <v-card>\n      <v-card-title>\n        <span class=\"headline\">{{ title }}</span>\n        <v-spacer></v-spacer>\n      </v-card-title>\n      <v-card-text>\n        <div>\n          <h3>Seguimientos</h3>\n          <v-list>\n            <v-list-item\n              v-for=\"seguimiento in seguimientos\"\n              :key=\"seguimiento._id\"\n            >\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(seguimiento)\">{{\n                  seguimiento.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </div>\n        <div>\n          <h3>Asesorias</h3>\n          <v-list v-if=\"asesorias.length > 0\">\n            <v-list-item v-for=\"asesoria in asesorias\" :key=\"asesoria._id\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(asesoria)\">{{\n                  asesoria.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n          <div v-else>No Asesorias found</div>\n        </div>\n        <div>\n          <h3>Archivos</h3>\n          <v-list v-if=\"Object.keys(archivos).length > 0\">\n            <v-list-item v-for=\"(fileId, fileName) in archivos\" :key=\"fileId\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFileById(fileId)\">{{\n                  fileName\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n          <div v-else>No Archivos found</div>\n        </div>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-2\" text @click=\"closeDialog\">Close</v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nexport default {\n  name: 'FilesDialog',\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: 'Documentos'\n    }\n  },\n  data () {\n    return {\n      files: [],\n      seguimientos: [],\n      asesorias: [],\n      archivos: {}\n    }\n  },\n  computed: {\n    dialogVisible: {\n      get () {\n        return this.isOpen\n      },\n      set (value) {\n        this.$emit('update:isOpen', value)\n      }\n    }\n  },\n  watch: {\n    dialogVisible (newVal) {\n      if (newVal) {\n        this.loadFiles()\n        this.loadSeguimientos()\n        this.loadAsesorias()\n        this.loadArchivos()\n      }\n    }\n  },\n  methods: {\n    loadFiles () {\n      const fileIds = [this.student.file1, this.student.file2]\n      this.fetchFiles('uploads', fileIds).then((files) => {\n        this.files = files\n      })\n    },\n    loadSeguimientos () {\n      if (\n        this.student &&\n        this.student.seguimiento1 &&\n        this.student.seguimiento2 &&\n        this.student.seguimiento3\n      ) {\n        const seguimientoIds = [\n          this.student.seguimiento1,\n          this.student.seguimiento2,\n          this.student.seguimiento3\n        ]\n        this.fetchFiles('uploads', seguimientoIds).then((files) => {\n          this.seguimientos = files\n        })\n      }\n    },\n    loadAsesorias () {\n      const asesoriaIds = this.student.archivosAsesorias || []\n      if (asesoriaIds.length > 0) {\n        this.fetchFiles('asesoria', asesoriaIds).then((files) => {\n          this.asesorias = files\n        })\n      } else {\n        this.asesorias = []\n      }\n    },\n    loadArchivos () {\n      this.archivos = this.student.archivos || {}\n    },\n    closeDialog () {\n      this.dialogVisible = false\n    },\n    async fetchFiles (collection, fileIds) {\n      try {\n        const files = await Promise.all(\n          fileIds.map(async (fileId) => {\n            const response = await fetch(\n              `http://localhost:3000/files/${collection}/${fileId}`\n            )\n            if (!response.ok) {\n              throw new Error(`Failed to fetch file with ID ${fileId}`)\n            }\n            const fileData = await response.json()\n            return {\n              _id: fileId,\n              filename: fileData.filename,\n              data: fileData.data\n            }\n          })\n        )\n        return files\n      } catch (error) {\n        console.error('Error fetching files:', error)\n        return []\n      }\n    },\n    downloadFile (file) {\n      const link = document.createElement('a')\n      link.href = `data:application/pdf;base64,${file.data}`\n      link.download = file.filename\n      link.click()\n    },\n    async downloadFileById (fileId) {\n      try {\n        const response = await fetch(\n          `http://localhost:3000/files/uploads/${fileId}`\n        )\n        if (!response.ok) {\n          throw new Error(`Failed to fetch file with ID ${fileId}`)\n        }\n        const fileData = await response.json()\n        const link = document.createElement('a')\n        link.href = `data:application/pdf;base64,${fileData.data}`\n        link.download = fileData.filename\n        link.click()\n      } catch (error) {\n        console.error('Error downloading file:', error)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.headline {\n  font-weight: bold;\n}\n.v-list-item-title {\n  cursor: pointer;\n}\n</style>\n"],"mappings":"AAoEA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,MAAM;MACZF,QAAQ,EAAE;IACZ,CAAC;IACDG,KAAK,EAAE;MACLL,IAAI,EAAEM,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,CAAC;IACb;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAa,EAAE;MACbC,GAAEA,CAAA,EAAK;QACL,OAAO,IAAI,CAAChB,MAAK;MACnB,CAAC;MACDiB,GAAEA,CAAGC,KAAK,EAAE;QACV,IAAI,CAACC,KAAK,CAAC,eAAe,EAAED,KAAK;MACnC;IACF;EACF,CAAC;EACDE,KAAK,EAAE;IACLL,aAAYA,CAAGM,MAAM,EAAE;MACrB,IAAIA,MAAM,EAAE;QACV,IAAI,CAACC,SAAS,CAAC;QACf,IAAI,CAACC,gBAAgB,CAAC;QACtB,IAAI,CAACC,aAAa,CAAC;QACnB,IAAI,CAACC,YAAY,CAAC;MACpB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPJ,SAAQA,CAAA,EAAK;MACX,MAAMK,OAAM,GAAI,CAAC,IAAI,CAACvB,OAAO,CAACwB,KAAK,EAAE,IAAI,CAACxB,OAAO,CAACyB,KAAK;MACvD,IAAI,CAACC,UAAU,CAAC,SAAS,EAAEH,OAAO,CAAC,CAACI,IAAI,CAAErB,KAAK,IAAK;QAClD,IAAI,CAACA,KAAI,GAAIA,KAAI;MACnB,CAAC;IACH,CAAC;IACDa,gBAAeA,CAAA,EAAK;MAClB,IACE,IAAI,CAACnB,OAAM,IACX,IAAI,CAACA,OAAO,CAAC4B,YAAW,IACxB,IAAI,CAAC5B,OAAO,CAAC6B,YAAW,IACxB,IAAI,CAAC7B,OAAO,CAAC8B,YAAW,EACxB;QACA,MAAMC,cAAa,GAAI,CACrB,IAAI,CAAC/B,OAAO,CAAC4B,YAAY,EACzB,IAAI,CAAC5B,OAAO,CAAC6B,YAAY,EACzB,IAAI,CAAC7B,OAAO,CAAC8B,YAAW,CAC1B;QACA,IAAI,CAACJ,UAAU,CAAC,SAAS,EAAEK,cAAc,CAAC,CAACJ,IAAI,CAAErB,KAAK,IAAK;UACzD,IAAI,CAACC,YAAW,GAAID,KAAI;QAC1B,CAAC;MACH;IACF,CAAC;IACDc,aAAYA,CAAA,EAAK;MACf,MAAMY,WAAU,GAAI,IAAI,CAAChC,OAAO,CAACiC,iBAAgB,IAAK,EAAC;MACvD,IAAID,WAAW,CAACE,MAAK,GAAI,CAAC,EAAE;QAC1B,IAAI,CAACR,UAAU,CAAC,UAAU,EAAEM,WAAW,CAAC,CAACL,IAAI,CAAErB,KAAK,IAAK;UACvD,IAAI,CAACE,SAAQ,GAAIF,KAAI;QACvB,CAAC;MACH,OAAO;QACL,IAAI,CAACE,SAAQ,GAAI,EAAC;MACpB;IACF,CAAC;IACDa,YAAWA,CAAA,EAAK;MACd,IAAI,CAACZ,QAAO,GAAI,IAAI,CAACT,OAAO,CAACS,QAAO,IAAK,CAAC;IAC5C,CAAC;IACD0B,WAAUA,CAAA,EAAK;MACb,IAAI,CAACxB,aAAY,GAAI,KAAI;IAC3B,CAAC;IACD,