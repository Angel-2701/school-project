{"ast":null,"code":"export default {\n  name: 'FilesDialog',\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: 'Documentos'\n    }\n  },\n  data() {\n    return {\n      files: [],\n      seguimientos: [],\n      asesorias: [] // Example placeholder data for Asesorias\n    };\n  },\n  computed: {\n    dialogVisible: {\n      get() {\n        return this.isOpen;\n      },\n      set(value) {\n        this.$emit('update:isOpen', value);\n      }\n    }\n  },\n  watch: {\n    dialogVisible(newVal) {\n      if (newVal) {\n        this.loadFiles();\n        this.loadSeguimientos();\n        this.loadAsesorias();\n      }\n    }\n  },\n  methods: {\n    loadFiles() {\n      const fileIds = [this.student.file1, this.student.file2];\n      this.fetchFiles('uploads', fileIds).then(files => {\n        this.files = files;\n      });\n    },\n    loadSeguimientos() {\n      if (this.student && this.student.seguimiento1 && this.student.seguimiento2 && this.student.seguimiento3) {\n        const seguimientoIds = [this.student.seguimiento1, this.student.seguimiento2, this.student.seguimiento3];\n        this.fetchFiles('uploads', seguimientoIds).then(files => {\n          this.seguimientos = files;\n        });\n      }\n    },\n    loadAsesorias() {\n      const asesoriaIds = this.student.archivosAsesorias || [];\n      if (asesoriaIds.length > 0) {\n        this.fetchFiles('asesoria', asesoriaIds).then(files => {\n          this.asesorias = files;\n        });\n      } else {\n        this.asesorias = [];\n      }\n    },\n    closeDialog() {\n      this.dialogVisible = false;\n    },\n    async fetchFiles(collection, fileIds) {\n      try {\n        const files = await Promise.all(fileIds.map(async fileId => {\n          const response = await fetch(`http://localhost:3000/files/${collection}/${fileId}`);\n          if (!response.ok) {\n            throw new Error(`Failed to fetch file with ID ${fileId}`);\n          }\n          const fileData = await response.json();\n          return {\n            _id: fileId,\n            filename: fileData.filename,\n            data: fileData.data\n          };\n        }));\n        return files;\n      } catch (error) {\n        console.error('Error fetching files:', error);\n        return [];\n      }\n    },\n    downloadFile(file) {\n      const link = document.createElement('a');\n      link.href = `data:application/pdf;base64,${file.data}`;\n      link.download = file.filename;\n      link.click();\n    }\n  }\n};","map":{"version":3,"names":["name","props","isOpen","type","Boolean","required","student","Object","title","String","default","data","files","seguimientos","asesorias","computed","dialogVisible","get","set","value","$emit","watch","newVal","loadFiles","loadSeguimientos","loadAsesorias","methods","fileIds","file1","file2","fetchFiles","then","seguimiento1","seguimiento2","seguimiento3","seguimientoIds","asesoriaIds","archivosAsesorias","length","closeDialog","collection","Promise","all","map","fileId","response","fetch","ok","Error","fileData","json","_id","filename","error","console","downloadFile","file","link","document","createElement","href","download","click"],"sources":["/Users/angel/school-project/my-app/src/components/FilesDialog.vue"],"sourcesContent":["<template>\n  <v-dialog v-model=\"dialogVisible\" max-width=\"600\">\n    <v-card>\n      <v-card-title>\n        <span class=\"headline\">{{ title }}</span>\n        <v-spacer></v-spacer>\n        <v-btn icon @click=\"closeDialog\">\n          <v-icon>mdi-close</v-icon>\n        </v-btn>\n      </v-card-title>\n      <v-card-text>\n        <div>\n          <h3>Seguimientos</h3>\n          <v-list>\n            <v-list-item\n              v-for=\"seguimiento in seguimientos\"\n              :key=\"seguimiento._id\"\n            >\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(seguimiento)\">{{\n                  seguimiento.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </div>\n        <div>\n          <h3>Asesorias</h3>\n          <v-list v-if=\"asesorias.length > 0\">\n            <v-list-item v-for=\"asesoria in asesorias\" :key=\"asesoria._id\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(asesoria)\">{{\n                  asesoria.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n          <div v-else>No Asesorias found</div>\n        </div>\n        <div>\n          <h3>Archivos</h3>\n          <v-list v-if=\"files.length > 0\">\n            <v-list-item v-for=\"file in files\" :key=\"file._id\">\n              <v-list-item-icon>\n                <v-icon>mdi-file-pdf</v-icon>\n              </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title @click=\"downloadFile(file)\">{{\n                  file.filename\n                }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list>\n        </div>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-2\" text @click=\"closeDialog\">Close</v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nexport default {\n  name: 'FilesDialog',\n  props: {\n    isOpen: {\n      type: Boolean,\n      required: true\n    },\n    student: {\n      type: Object,\n      required: true\n    },\n    title: {\n      type: String,\n      default: 'Documentos'\n    }\n  },\n  data () {\n    return {\n      files: [],\n      seguimientos: [],\n      asesorias: [] // Example placeholder data for Asesorias\n    }\n  },\n  computed: {\n    dialogVisible: {\n      get () {\n        return this.isOpen\n      },\n      set (value) {\n        this.$emit('update:isOpen', value)\n      }\n    }\n  },\n  watch: {\n    dialogVisible (newVal) {\n      if (newVal) {\n        this.loadFiles()\n        this.loadSeguimientos()\n        this.loadAsesorias()\n      }\n    }\n  },\n  methods: {\n    loadFiles () {\n      const fileIds = [this.student.file1, this.student.file2]\n      this.fetchFiles('uploads', fileIds).then((files) => {\n        this.files = files\n      })\n    },\n    loadSeguimientos () {\n      if (\n        this.student &&\n        this.student.seguimiento1 &&\n        this.student.seguimiento2 &&\n        this.student.seguimiento3\n      ) {\n        const seguimientoIds = [\n          this.student.seguimiento1,\n          this.student.seguimiento2,\n          this.student.seguimiento3\n        ]\n        this.fetchFiles('uploads', seguimientoIds).then((files) => {\n          this.seguimientos = files\n        })\n      }\n    },\n    loadAsesorias () {\n      const asesoriaIds = this.student.archivosAsesorias || []\n      if (asesoriaIds.length > 0) {\n        this.fetchFiles('asesoria', asesoriaIds).then((files) => {\n          this.asesorias = files\n        })\n      } else {\n        this.asesorias = []\n      }\n    },\n    closeDialog () {\n      this.dialogVisible = false\n    },\n    async fetchFiles (collection, fileIds) {\n      try {\n        const files = await Promise.all(\n          fileIds.map(async (fileId) => {\n            const response = await fetch(\n              `http://localhost:3000/files/${collection}/${fileId}`\n            )\n            if (!response.ok) {\n              throw new Error(`Failed to fetch file with ID ${fileId}`)\n            }\n            const fileData = await response.json()\n            return {\n              _id: fileId,\n              filename: fileData.filename,\n              data: fileData.data\n            }\n          })\n        )\n        return files\n      } catch (error) {\n        console.error('Error fetching files:', error)\n        return []\n      }\n    },\n    downloadFile (file) {\n      const link = document.createElement('a')\n      link.href = `data:application/pdf;base64,${file.data}`\n      link.download = file.filename\n      link.click()\n    }\n  }\n}\n</script>\n\n<style scoped>\n.headline {\n  font-weight: bold;\n}\n.v-list-item-title {\n  cursor: pointer;\n}\n</style>\n"],"mappings":"AAsEA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,OAAO,EAAE;MACPH,IAAI,EAAEI,MAAM;MACZF,QAAQ,EAAE;IACZ,CAAC;IACDG,KAAK,EAAE;MACLL,IAAI,EAAEM,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,EAAC,CAAE;IAChB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAa,EAAE;MACbC,GAAEA,CAAA,EAAK;QACL,OAAO,IAAI,CAACf,MAAK;MACnB,CAAC;MACDgB,GAAEA,CAAGC,KAAK,EAAE;QACV,IAAI,CAACC,KAAK,CAAC,eAAe,EAAED,KAAK;MACnC;IACF;EACF,CAAC;EACDE,KAAK,EAAE;IACLL,aAAYA,CAAGM,MAAM,EAAE;MACrB,IAAIA,MAAM,EAAE;QACV,IAAI,CAACC,SAAS,CAAC;QACf,IAAI,CAACC,gBAAgB,CAAC;QACtB,IAAI,CAACC,aAAa,CAAC;MACrB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPH,SAAQA,CAAA,EAAK;MACX,MAAMI,OAAM,GAAI,CAAC,IAAI,CAACrB,OAAO,CAACsB,KAAK,EAAE,IAAI,CAACtB,OAAO,CAACuB,KAAK;MACvD,IAAI,CAACC,UAAU,CAAC,SAAS,EAAEH,OAAO,CAAC,CAACI,IAAI,CAAEnB,KAAK,IAAK;QAClD,IAAI,CAACA,KAAI,GAAIA,KAAI;MACnB,CAAC;IACH,CAAC;IACDY,gBAAeA,CAAA,EAAK;MAClB,IACE,IAAI,CAAClB,OAAM,IACX,IAAI,CAACA,OAAO,CAAC0B,YAAW,IACxB,IAAI,CAAC1B,OAAO,CAAC2B,YAAW,IACxB,IAAI,CAAC3B,OAAO,CAAC4B,YAAW,EACxB;QACA,MAAMC,cAAa,GAAI,CACrB,IAAI,CAAC7B,OAAO,CAAC0B,YAAY,EACzB,IAAI,CAAC1B,OAAO,CAAC2B,YAAY,EACzB,IAAI,CAAC3B,OAAO,CAAC4B,YAAW,CAC1B;QACA,IAAI,CAACJ,UAAU,CAAC,SAAS,EAAEK,