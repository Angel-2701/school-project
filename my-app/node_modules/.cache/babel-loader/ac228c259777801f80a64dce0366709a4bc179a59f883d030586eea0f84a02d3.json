{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      students: [],\n      // Change users to students\n      tableHeaders: [],\n      sidebarItems: [{\n        title: \"Projects\",\n        icon: \"mdi-folder-outline\"\n      }, {\n        title: \"Students\",\n        icon: \"mdi-account-plus-outline\"\n      }, {\n        title: \"Create Teacher\",\n        icon: \"mdi-teacher\"\n      }],\n      userName: \"John Doe\",\n      drawer: false,\n      editDialog: false,\n      editedUserName: \"\",\n      editedUserId: \"\",\n      createDialog: false,\n      newUser: {\n        name: \"\",\n        email: \"\"\n        // Add more fields as needed\n      }\n    };\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/students\"); // Fetch students data\n        this.students = response.data; // Assign fetched students data\n\n        // Generate table headers based on the keys of the first student\n        if (this.students.length > 0) {\n          const firstStudent = this.students[0];\n          const keys = Object.keys(firstStudent).map(key => ({\n            text: key.charAt(0).toUpperCase() + key.slice(1),\n            value: key\n          }));\n          this.tableHeaders = keys;\n        }\n      } catch (error) {\n        console.error(\"Error fetching students:\", error);\n      }\n    },\n    navigate(index) {\n      switch (index) {\n        case 0:\n          this.$router.push(\"/admin\");\n          break;\n        case 1:\n          this.$router.push(\"/students\");\n          break;\n        case 2:\n          this.$router.push(\"/create-teacher\");\n          break;\n        default:\n          break;\n      }\n    },\n    logout() {\n      // Logic to logout the user\n    },\n    editUser(user) {\n      this.editedUserName = user.name;\n      this.editedUserId = user._id;\n      this.editDialog = true;\n    },\n    saveEditedUser() {\n      // Logic to save the edited user name\n      console.log(\"Saving edited user name:\", this.editedUserName);\n      this.editDialog = false;\n    },\n    cancelEdit() {\n      this.editDialog = false;\n    },\n    openCreateDialog() {\n      this.createDialog = true;\n    },\n    saveNewUser() {\n      axios.post(\"http://localhost:3000/users\", this.newUser).then(response => {\n        if (response.status === 201) {\n          console.log(\"User created successfully:\", response.data);\n          this.fetchData();\n        } else {\n          console.error(\"Failed to create user:\", response.data);\n        }\n      }).catch(error => {\n        console.error(\"Error creating user:\", error);\n      });\n      this.createDialog = false;\n    },\n    cancelCreate() {\n      this.createDialog = false;\n    }\n  },\n  mounted() {\n    this.fetchData(); // Fetch students data when the component is mounted\n  }\n};","map":{"version":3,"names":["axios","data","students","tableHeaders","sidebarItems","title","icon","userName","drawer","editDialog","editedUserName","editedUserId","createDialog","newUser","name","email","methods","fetchData","response","get","length","firstStudent","keys","Object","map","key","text","charAt","toUpperCase","slice","value","error","console","navigate","index","$router","push","logout","editUser","user","_id","saveEditedUser","log","cancelEdit","openCreateDialog","saveNewUser","post","then","status","catch","cancelCreate","mounted"],"sources":["/Users/angel/project-system/my-app/src/components/StudentsPage.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-container fluid>\n      <!-- Top Bar -->\n      <v-app-bar app color=\"blue darken-2\" dark>\n        <!-- Burger menu icon -->\n        <v-btn icon @click=\"drawer = !drawer\">\n          <v-icon>mdi-menu</v-icon>\n        </v-btn>\n        <!-- Title -->\n        <v-toolbar-title>{{ userName }}</v-toolbar-title>\n        <v-spacer></v-spacer>\n        <!-- Logout button -->\n        <v-btn icon @click=\"logout\">\n          <v-icon>mdi-logout</v-icon>\n        </v-btn>\n      </v-app-bar>\n\n      <!-- Sidebar -->\n      <v-navigation-drawer app v-model=\"drawer\" color=\"blue darken-2\" dark>\n        <v-list dense>\n          <v-list-item\n            v-for=\"(item, index) in sidebarItems\"\n            :key=\"index\"\n            @click=\"navigate(index)\"\n          >\n            <v-list-item-action>\n              <v-icon>{{ item.icon }}</v-icon>\n            </v-list-item-action>\n            <v-list-item-content>\n              <v-list-item-title>{{ item.title }}</v-list-item-title>\n            </v-list-item-content>\n          </v-list-item>\n        </v-list>\n      </v-navigation-drawer>\n\n      <v-main>\n        <!-- Main content -->\n        <v-col cols=\"10\">\n          <v-card>\n            <!-- Users Data -->\n            <v-data-table\n              v-if=\"students.length > 0\"\n              :items=\"students\"\n              align=\"center\"\n            >\n              <template v-slot:top>\n                <v-toolbar flat color=\"blue darken-2\">\n                  <v-toolbar-title>Students</v-toolbar-title>\n                  <v-divider class=\"mx-4\" inset vertical></v-divider>\n                  <v-spacer></v-spacer>\n                  <v-btn icon color=\"blue darken-2\" @click=\"openCreateDialog\">\n                    <v-icon>mdi-plus</v-icon>\n                  </v-btn>\n                </v-toolbar>\n              </template>\n              <template v-slot:item=\"{ item }\">\n                <tr>\n                  <td v-for=\"(value, key) in item\" :key=\"key\">\n                    {{ value }}\n                  </td>\n                  <td>\n                    <v-btn\n                      icon\n                      small\n                      color=\"blue darken-2\"\n                      @click=\"editUser(item)\"\n                      style=\"width: 30px; height: 30px; margin-right: 5px\"\n                    >\n                      <v-icon style=\"font-size: 18px\">mdi-pencil</v-icon>\n                    </v-btn>\n                    <v-btn\n                      icon\n                      small\n                      color=\"red darken-2\"\n                      @click=\"deleteUser(item)\"\n                      style=\"width: 30px; height: 30px; margin-right: 5px\"\n                    >\n                      <v-icon style=\"font-size: 18px\">mdi-delete</v-icon>\n                    </v-btn>\n                  </td>\n                </tr>\n              </template>\n            </v-data-table>\n          </v-card>\n        </v-col>\n      </v-main>\n\n      <!-- Edit Dialog -->\n      <v-dialog v-model=\"editDialog\" max-width=\"500\">\n        <v-card>\n          <v-card-title>Edit User Name</v-card-title>\n          <v-card-text>\n            <v-text-field\n              v-model=\"editedUserName\"\n              label=\"User Name\"\n            ></v-text-field>\n          </v-card-text>\n          <v-card-actions>\n            <v-btn color=\"blue darken-2\" @click=\"saveEditedUser\">Save</v-btn>\n            <v-btn @click=\"cancelEdit\">Cancel</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n\n      <!-- Create Dialog -->\n      <v-dialog v-model=\"createDialog\" max-width=\"500\">\n        <v-card>\n          <v-card-title>Create New User</v-card-title>\n          <v-card-text>\n            <v-text-field\n              v-model=\"newUser.name\"\n              label=\"User Name\"\n            ></v-text-field>\n            <v-text-field v-model=\"newUser.email\" label=\"Email\"></v-text-field>\n            <!-- Add more fields as needed -->\n          </v-card-text>\n          <v-card-actions>\n            <v-btn color=\"blue darken-2\" @click=\"saveNewUser\">Save</v-btn>\n            <v-btn @click=\"cancelCreate\">Cancel</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-container>\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      students: [], // Change users to students\n      tableHeaders: [],\n      sidebarItems: [\n        { title: \"Projects\", icon: \"mdi-folder-outline\" },\n        { title: \"Students\", icon: \"mdi-account-plus-outline\" },\n        { title: \"Create Teacher\", icon: \"mdi-teacher\" },\n      ],\n      userName: \"John Doe\",\n      drawer: false,\n      editDialog: false,\n      editedUserName: \"\",\n      editedUserId: \"\",\n      createDialog: false,\n      newUser: {\n        name: \"\",\n        email: \"\",\n        // Add more fields as needed\n      },\n    };\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/students\"); // Fetch students data\n        this.students = response.data; // Assign fetched students data\n\n        // Generate table headers based on the keys of the first student\n        if (this.students.length > 0) {\n          const firstStudent = this.students[0];\n          const keys = Object.keys(firstStudent).map((key) => ({\n            text: key.charAt(0).toUpperCase() + key.slice(1),\n            value: key,\n          }));\n          this.tableHeaders = keys;\n        }\n      } catch (error) {\n        console.error(\"Error fetching students:\", error);\n      }\n    },\n\n    navigate(index) {\n      switch (index) {\n        case 0:\n          this.$router.push(\"/admin\");\n          break;\n        case 1:\n          this.$router.push(\"/students\");\n          break;\n        case 2:\n          this.$router.push(\"/create-teacher\");\n          break;\n        default:\n          break;\n      }\n    },\n    logout() {\n      // Logic to logout the user\n    },\n    editUser(user) {\n      this.editedUserName = user.name;\n      this.editedUserId = user._id;\n      this.editDialog = true;\n    },\n    saveEditedUser() {\n      // Logic to save the edited user name\n      console.log(\"Saving edited user name:\", this.editedUserName);\n      this.editDialog = false;\n    },\n    cancelEdit() {\n      this.editDialog = false;\n    },\n    openCreateDialog() {\n      this.createDialog = true;\n    },\n    saveNewUser() {\n      axios\n        .post(\"http://localhost:3000/users\", this.newUser)\n        .then((response) => {\n          if (response.status === 201) {\n            console.log(\"User created successfully:\", response.data);\n            this.fetchData();\n          } else {\n            console.error(\"Failed to create user:\", response.data);\n          }\n        })\n        .catch((error) => {\n          console.error(\"Error creating user:\", error);\n        });\n      this.createDialog = false;\n    },\n    cancelCreate() {\n      this.createDialog = false;\n    },\n  },\n  mounted() {\n    this.fetchData(); // Fetch students data when the component is mounted\n  },\n};\n</script>\n\n<style scoped>\n/* Adjustments for burger menu icon color */\n.v-btn i {\n  color: white;\n}\n</style>\n"],"mappings":";AAgIA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MAAE;MACdC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,CACZ;QAAEC,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAqB,CAAC,EACjD;QAAED,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE;MAA2B,CAAC,EACvD;QAAED,KAAK,EAAE,gBAAgB;QAAEC,IAAI,EAAE;MAAc,CAAC,CACjD;MACDC,QAAQ,EAAE,UAAU;MACpBC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,KAAK;MACjBC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,KAAK;MACnBC,OAAO,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;QACP;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAG,CAAC,gCAAgC,CAAC,EAAE;QACpE,IAAI,CAACjB,QAAO,GAAIgB,QAAQ,CAACjB,IAAI,EAAE;;QAE/B;QACA,IAAI,IAAI,CAACC,QAAQ,CAACkB,MAAK,GAAI,CAAC,EAAE;UAC5B,MAAMC,YAAW,GAAI,IAAI,CAACnB,QAAQ,CAAC,CAAC,CAAC;UACrC,MAAMoB,IAAG,GAAIC,MAAM,CAACD,IAAI,CAACD,YAAY,CAAC,CA